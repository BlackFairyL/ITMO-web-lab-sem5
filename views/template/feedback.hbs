<section class="content__page">
  <script src="JS/loadJSON.js" type="text/javascript"></script>
  <script src="JS/addFeedback.js" type="text/javascript"></script>
  <div class="chat" id="chat">
    <div>
      <input class="myInput" id="feedback" type="text" required />
      <button class="feedbtn" onclick="send()">Send</button>
    </div>
    <div>
      <ul id="sent_message"></ul>
    </div>
  </div>
  <pre id="json"></pre>
  <div class="constructor__table">
    <table class="table">
      <thead>
      <tr id="notdel">
        <th>Имя пользователя</th>
        <th>Заголовок</th>
        <th>Отзыв</th>
      </tr>
      </thead>
      <tbody></tbody>
    </table>
  </div>
  <template id="template">
    <tr>
      <td id="template_authorId"></td>
      <td id="template_title"></td>
      <td id="template_content"></td>
    </tr>
  </template>
</section>